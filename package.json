{
  "name": "dev-greyatom-ide",
  "version": "2.0.13",
  "atomVersion": "1.14.2",
  "treeVersion": "1.2.10",
  "languagerVersion": "0.4.1",
  "hydrogenVersion": "1.9.0",
  "main": "./lib/commit-live",
  "urlMain": "./lib/url-handler",
  "description": "Dev version of Commit.Live IDE",
  "scripts": {
    "start": "./node_modules/gulp/bin/gulp.js"
  },
  "keywords": [],
  "repository": "https://github.com/commit-live-admin/dev-greyatom-ide",
  "pkgRepository": "https://github.com/commit-live-admin/dev-greyatom-ide/",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "consumedServices": {
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    }
  },
  "dependencies": {
    "atom-select-list": "^0.0.14",
    "atom-socket": "0.0.9",
    "atom-space-pen-views": "2.1.0",
    "dotenv": "2.0.0",
    "fuzzaldrin": "^2.0",
    "fuzzaldrin-plus": "^0.4.1",
    "learn-term.js": "0.0.7",
    "lodash": "^4.17.4",
    "page-bus": "3.0.1",
    "semver": "5.3.0",
    "socket.io": "^1.7.2",
    "underscore-plus": "1.6.6",
    "utf8": "2.1.1"
  },
  "packageDependencies": {
    "greyatom-tree-view": "1.3.24",
    "tablr": "1.8.3",
    "tool-bar": "1.1.1",
    "greyatom-ide-welcome": "0.0.9",
    "greyatom-ide-background-tips": "0.0.3"
  },
  "configSchema": {
    "notifier": {
      "type": "boolean",
      "default": true,
      "title": "Greyatom Status Notifications",
      "description": "Receive desktop notifications that correspond to the lights on Lgreyatom"
    },
    "terminalFontColor": {
      "type": "string",
      "default": "#ffffff",
      "title": "Terminal Font Color",
      "description": "A valid CSS color value which will be used to style your terminal's font"
    },
    "terminalBackgroundColor": {
      "type": "string",
      "default": "#000000",
      "title": "Terminal Background Color",
      "description": "A valid CSS color value which will be used to style your terminal's background"
    },
    "apiEndpoint": {
      "type": "string",
      "default": "http://api.greyatom.com/v2",
      "title": "Commit Live Endpoint",
      "description": "Endpoint for the IDE to use for User Authentication"
    }
  },
  "devDependencies": {
    "decompress": "4.0.0",
    "del": "2.2.2",
    "gulp": "3.9.1",
    "gulp-util": "3.0.7",
    "request": "2.75.0",
    "run-sequence": "1.2.2",
    "shelljs": "0.7.3",
    "ssh2": "0.5.0"
  },
  "optionalDependencies": {
    "appdmg": "^0.5.2"
  }
}
